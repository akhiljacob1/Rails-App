<h1>Bookreviews#index</h1>
<h3>Bookreviews</h3>

<ul>
  <% @book_reviews.each do |book_review| %>
    <li>
      <% results = GoogleBooks.search("isbn:#{book_review.book_id}") %>
      <%= link_to results.first.title, book_review %>
    </li>
  <% end %>
</ul>

<%= link_to "New Bookreview", search_path %>